#include <metal.hpp>

template<int> struct x;

<% env[:k].times do |k| %>

using <%= "xs#{k}" %> = metal::list<
    <%= ((k*n+1)..(k*n+n)).map { |i| "x<#{i}>" }.join(', ') %>
>;

#if defined(METABENCH)
using <%= "result#{k}" %> = metal::insert<<%= "xs#{k}" %>, metal::number<<%= k*n/env[:k] %>>, void>;
#endif

<% end %>

int main() {}
