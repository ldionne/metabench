#include <boost/mp11.hpp>
using namespace boost::mp11;

template<int> struct x;

<% env[:k].times do |k| %>

using <%= "xs#{k}" %> = mp_list<
    <%= ((k*n+1)..(k*n+n)).map { |i| "x<#{i}>" }.join(', ') %>
>;

    <% (env[:p]*n/100).times do |p| %>
    #if defined(METABENCH)
    using <%= "result#{p}#{k}" %> = mp_at_c<<%= "xs#{k}" %>, <%= p*100/env[:p] %>>;
    #endif
    <% end %>
<% end %>

int main() {}
