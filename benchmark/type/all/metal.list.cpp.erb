#include <metal.hpp>

template<typename N>
using is_positive = metal::number<(N::type::value > 0)>;

using list = metal::list<
    <%= (1..n).map { |i| "metal::number<#{i}>" }.join(', ') %>
>;

#if defined(METABENCH)
using result = metal::all<list, metal::lambda<is_positive>>;
#endif

