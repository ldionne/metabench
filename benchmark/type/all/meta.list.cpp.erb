#include <meta/meta.hpp>

struct is_positive {
    template<typename N>
    using invoke = meta::bool_<(N::type::value > 0)>;
};

using list = meta::list<
    <%= (1..n).map { |i| "meta::int_<#{i}>" }.join(', ') %>
>;

#if defined(METABENCH)
using result = meta::all_of<list, is_positive>;
#endif

