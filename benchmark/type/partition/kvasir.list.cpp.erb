#include <mpl.hpp>

template<typename N>
using is_even = std::integral_constant<int,N::value % 2 == 0>;

<% env[:k].times do |k| %>

using <%= "xs#{k}" %> = metal::list<
    <%= ((k*n+1)..(k*n+n)).map { |i| "std::integral_constant<int,#{i}>" }.join(', ') %>
>;

#if defined(METABENCH)
using <%= "result#{k}" %> = mpl::partition<is_even,<%= "xs#{k}" %>>;
#endif

<% end %>

int main() {}
