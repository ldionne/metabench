#include <meta/meta.hpp>

<% env[:k].times do |k| %>

using <%= "xs#{k}" %> = meta::list<<%=
    xs = ((k*n+1)..(k*n+n)).map { |i| "meta::int_<#{i}>" }
    rng = Random.new(k)
    xs.shuffle(random: rng).join(', ')
%>>;

#if defined(METABENCH)
using <%= "result#{k}" %> = meta::sort<<%= "xs#{k}" %>, meta::quote<meta::less>>;
#endif

<% end %>

int main() { }
