#include <kvasir/mpl/mpl.hpp>
namespace mpl = kvasir::mpl;

template<typename N>
using is_even = mpl::int_<N::value % 2 == 0>;

<% env[:k].times do |k| %>

using <%= "xs#{k}" %> = mpl::list<
    <%= ((k*n+1)..(k*n+n)).map { |i| "mpl::int_<#{i}>" }.join(', ') %>
>;

#if defined(METABENCH)
using <%= "result#{k}" %> = mpl::replace_if<<%= "xs#{k}" %>, void, is_even>;
#endif

<% end %>

int main() {}
