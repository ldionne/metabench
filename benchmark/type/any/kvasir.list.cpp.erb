#include <mpl.hpp>

template<typename N>
using is_last = std::integral_constant<int,N::value == <%= n %>>;

<% env[:k].times do |k| %>

using <%= "xs#{k}" %> = mpl::list<
    <%= ((k*n+1)..(k*n+n)).map { |i| "std::integral_constant<int,#{i}>" }.join(', ') %>
>;

#if defined(METABENCH)
using <%= "result#{k}" %> = mpl::any<is_last,<%= "xs#{k}" %>>;
#endif

<% end %>

int main() {}
