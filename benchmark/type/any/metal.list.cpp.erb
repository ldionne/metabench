#include <metal.hpp>

template<typename N>
using is_last = metal::number<N::type::value == <%= n %>>;

<% env[:k].times do |k| %>

using <%= "xs#{k}" %> = metal::list<
    <%= ((k*n+1)..(k*n+n)).map { |i| "metal::number<#{i}>" }.join(', ') %>
>;

#if defined(METABENCH)
using <%= "result#{k}" %> = metal::any_of<<%= "xs#{k}" %>, metal::lambda<is_last>>;
#endif

<% end %>

int main() {}
