#include <metal.hpp>

template<typename N>
using is_last = metal::number<N::type::value == <%= n %>>;

using list = metal::list<
    <%= (1..n).map { |i| "metal::number<#{i}>" }.join(', ') %>
>;

#if defined(METABENCH)
using result = metal::any<list, metal::lambda<is_last>>;
#endif

int main() { }
