#include <boost/mp11.hpp>
using namespace boost::mp11;

template<typename X>
using f = X;

template<int> struct x;

<% env[:k].times do |k| %>

using <%= "xs#{k}" %> = mp_list<
    <%= ((k*n+1)..(k*n+n)).map { |i| "x<#{i}>" }.join(', ') %>
>;

#if defined(METABENCH)
using <%= "result#{k}" %> = mp_transform<f, <%= "xs#{k}" %>>;
#endif

<% end %>

int main() {}
