#include <kvasir/mpl/mpl.hpp>
namespace mpl = kvasir::mpl;


template<int X>
struct equals_to {
  template <typename Y>
  using apply = mpl::bool_<X == Y::value>;
};

<% env[:k].times do |k| %>

<% ints = ((k*n+1)..(k*n+n)).to_a %>

using <%= "xs#{k}" %> = mpl::list<
    <%= ints.map { |i| "mpl::int_<#{i}>" }.join(', ') %>
>;

#if defined(METABENCH)
using <%= "result#{k}" %> = mpl::find_if<<%= "xs#{k}" %>, equals_to<<%= ints.last || 0 %>>::apply>;
#endif

<% end %>

int main() {}
