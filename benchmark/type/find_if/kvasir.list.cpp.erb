#include <mpl.hpp>

using namespace kvasir;
template<typename N>
using is_last = mpl::int_<N::value == <%= n %>>;

<% env[:k].times do |k| %>

using <%= "xs#{k}" %> = mpl::list<
    <%= ((k*n+1)..(k*n+n)).map { |i| "mpl::int_<#{i}>" }.join(', ') %>
>;

#if defined(METABENCH)
using <%= "result#{k}" %> = mpl::find_if<<%= "xs#{k}" %>,is_last>;
#endif

<% end %>

int main() {}
